Register A: 2024
Register B: 0
Register C: 0

Program: 0,3,5,4,3,0

program = [2,4,1,3,7,5,1,5,0,3,4,1,5,5,3,0]

def find(program, number):
    if program == []: return number
    for d in range(8):
        a = (number << 3) + d
        b = a % 8
        b = b ^ 3
        c = a >> b
        b = b ^ 5
        # a = number << 3 + b
        b = b ^ c
        if b % 8 == program[-1]:
            sub = find(program[:-1], a)
            if sub is None: continue
            return sub